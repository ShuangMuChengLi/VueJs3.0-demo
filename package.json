{
  "name": "project-init",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "api": "set NODE_ENV=development&& node ./bin/www",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "dockerNginxStart": "docker run --privileged=true -v /root/nginx/logs:/var/log/nginx/:rw -v /root/nginx/nginx.conf:/etc/nginx/nginx.conf:rw -v /root/dist:/root/dist:rw --name nginx -p 9998:80 -d nginx",
    "publish-to-server": "npm run build && npm run to-server",
    "to-server": "node ./bin/publish-script.js",
    "download-nginx-config": "node ./bin/download-nginx-config.js",
    "upload-nginx-config-by-docker": "node ./bin/upload-nginx-config-by-docker.js",
    "upload-nginx-config-not-docker": "node ./bin/upload-nginx-config-not-docker.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "echarts": "^4.7.0",
    "element-ui": "^2.13.1",
    "jquery": "^3.5.0",
    "moment": "^2.24.0",
    "ol": "^6.3.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "archiver": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "express": "~4.17.1",
    "http-errors": "~1.7.3",
    "http-proxy-middleware": "^0.20.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "node-ssh": "^8.0.0",
    "vue-template-compiler": "^2.6.11"
  }
}
