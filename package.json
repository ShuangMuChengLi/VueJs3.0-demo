    "name": "project-init",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "set NODE_ENV=dev&& node ./bin/www",
        "build": "set NODE_ENV=production&& webpack --config webpack.prod.js",
        "build:start": "set NODE_ENV=production&& node ./bin/www",
        "watch": "set NODE_ENV=watch&& webpack --progress --colors --watch  --config webpack.dev.js&&node ./bin/www",
